Algoritmo Ingresa_datos
	
	Definir nombre, apellidos, edad, pais Como Caracter;
	
	Escribir "Ingresa tu nombre";
	Leer nombre;
	
	Escribir "Ingresa tus apellidos";
	Leer apellidos;
	
	Escribir "Ingresa tu edad";
	Leer edad;
	
	Escribir "Ingresa tu pais";
	Leer pais;
	
	Imprimir "Tu nombre completo es ", nombre, " ", apellidos " tu edad es ", edad ". Vives en ", pais; 
	
FinAlgoritmo
